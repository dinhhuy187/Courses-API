{
  "JwtSettings": {
    "Key": "${JWT_KEY}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "ExpiryMinutes": "${JWT_EXPIRY_MINUTES}"
  },
  "ConnectionStrings": {
    "DefaultConnection": "${DB_CONNECTION}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "EmailSettings": {
    "SmtpServer": "${SMTP_SERVER}",
    "SmtpPort": "${SMTP_PORT}",
    "SenderEmail": "${SENDER_EMAIL}",
    "SenderPassword": "${SENDER_PASSWORD}",
    "SenderName": "${SENDER_NAME}"
  },
  "AllowedHosts": "*"
}